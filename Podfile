# Uncomment the next line to define a global platform for your project
 platform :ios, '14.0'

target 'meetups' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!
  
  # Important: Installing the latest versions to enable all the latest functionality
  # Note: NOT installing "Firebase" Pod - Installing separate Feature-Pods separately

  pod 'Firebase/Analytics', '~> 7.0.0'
  pod 'Firebase/Firestore', '~> 7.0.0'
  pod 'Firebase/Auth', '~> 7.0.0'
  pod 'Firebase/Functions', '~> 7.0.0'
  pod 'Firebase/Messaging', '~> 7.0.0'
  pod 'Firebase/Storage', '~> 7.0.0'
  pod 'Firebase/RemoteConfig', '~> 7.0.0'
  
  pod 'GoogleSignIn'

  pod 'FirebaseFirestoreSwift', '~> 7.0.0-beta'
end


